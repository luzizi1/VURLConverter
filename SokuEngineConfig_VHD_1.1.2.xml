                        <PrefixChekerStr>
                            <Value>encodeURIComponent('</Value>
                        </PrefixChekerStr>
                        <SuffixChekerStr>
                            <Value>')</Value>
                        </SuffixChekerStr>
                        <TargetURLTemplate>
                            <Value>%@</Value>
                        </TargetURLTemplate>
                    </Tyies>
                    <Tyies>
                        <PrefixChekerStr>
                            <Value>video=['</Value>
                        </PrefixChekerStr>
                        <SuffixChekerStr>
                            <Value>'</Value>
                        </SuffixChekerStr>
                        <TargetURLTemplate>
                            <Value>%@</Value>
                        </TargetURLTemplate>
                    </Tyies>
                    
                </Reponse>
            </ReponseHandler>
        </VideoUrlConvert>
